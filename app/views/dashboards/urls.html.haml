.col-md-12
  %table.table.table-striped
    %tr
      %th Key
      %th URL
      %th Actions
    - @urls.each do |url|
      %tr
        %td
          = url.key
        %td
          = url.original_url
        %td
          = link_to url, class: 'btn btn-success' do
            = icon 'eye'
          = link_to [:edit, url], class: 'btn btn-primary' do
            = icon 'pencil'
          = link_to url, method: :delete, class: 'btn btn-danger', data: {confirm: t('messages.delete_confirmation')} do
            = icon 'trash-o'


